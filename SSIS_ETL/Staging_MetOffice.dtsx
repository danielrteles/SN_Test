        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            string cityFileUrl = Dts.Variables["User::CityFileURL"].Value.ToString();
            string cityName = Dts.Variables["User::CityName"].Value.ToString();
            string localFileName = Dts.Variables["User::CurrentFileName"].Value.ToString();

            try
            {
                DownloadFile(localFileName);
                FindStartLine(localFileName);

            }
            catch (Exception ex)
            {
                // Logging why download failed
                Dts.Events.FireError(0, "Download File", "City " + cityName + " - Download failed: " + ex.Message, string.Empty, 0);

                // Quit Script Task unsuccesful
                Dts.TaskResult = (int)DTSExecResult.Failure;
            }

            Dts.TaskResult = (int)DTSExecResult.Success;

        }

        private void FindStartLine(string localFileName)
        {
            int startFrom = 0;
            string columnHeader = "yyyy  mm   tmax    tmin      af    rain     sun";

            string[] fileContent = File.ReadAllLines(localFileName);
            foreach (string line in fileContent)
            {
                startFrom++;
                if (line.Contains(columnHeader))
                    break;
            }

            //The files used in the example had the header on the 6th or 7th line.
            //Considering that the documentation link was broken, there's no way to check what's the limit for the text information
            //Therefore, I've considered a maximum of 15 lines as a criteria to identify invalid files
            if (startFrom > 15)
            {
                Dts.Events.FireError(0, "Remove Header", "Missing header: File " + localFileName + " has an invalid file structure.", string.Empty, 0);
            }

            Dts.Variables["User::StartFrom"].Value = startFrom;
        }

        private void DownloadFile(string localFileName)
        {
            Object httpSSISConnection = Dts.Connections["HTTP Connection"].AcquireConnection(null);
            HttpClientConnection clientConnection = new HttpClientConnection(httpSSISConnection);
            clientConnection.DownloadFile(localFileName, true);
        }

